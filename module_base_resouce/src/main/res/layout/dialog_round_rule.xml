<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.lib.basiclib.base.round.RoundRelativeLayout
        android:id="@+id/layoutRound"
        android:layout_width="290dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_height="350dp"
        android:layout_marginStart="@dimen/alivc_common_font_10"
        android:layout_marginEnd="@dimen/alivc_common_font_10"
        app:rv_backgroundColor="@color/white"
        app:rv_cornerRadius="@dimen/dp_15">

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/ic_pan_rule" />


    </com.lib.basiclib.base.round.RoundRelativeLayout>


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/closeImg"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="@+id/layoutRound"
        app:layout_constraintEnd_toEndOf="@+id/layoutRound"
        app:layout_constraintTop_toBottomOf="@+id/layoutRound"
        app:srcCompat="@mipmap/ic_close_round"/>
</androidx.constraintlayout.widget.ConstraintLayout>