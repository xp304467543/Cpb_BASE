<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:paddingStart="@dimen/dp_0"
    android:paddingEnd="10dp">

    <com.lib.basiclib.base.round.RoundTextView
        android:id="@+id/lineView"
        android:layout_width="4dp"
        android:layout_height="@dimen/dp_15"
        android:visibility="gone"
        app:rv_backgroundColor="@color/color_FF513E"
        app:rv_cornerRadius="@dimen/dp_10" />


    <com.lib.basiclib.base.round.RoundTextView
        android:id="@+id/tvBottomLine"
        android:layout_width="20dp"
        android:layout_height="5dp"
        android:layout_marginBottom="5dp"
        android:visibility="gone"
        app:rv_backgroundColor="@color/color_FFD4AF"
        app:rv_cornerRadius="@dimen/dp_5" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imgIcon"
        android:layout_width="@dimen/dp_25"
        android:layout_height="wrap_content" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvDesName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:drawablePadding="5dp"
        android:textColor="@color/color_333333"
        android:textSize="13sp"
        android:textStyle="bold"
        tools:text="我的" />


</LinearLayout>
